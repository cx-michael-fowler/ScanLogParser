Function Get-AllProjects {
    Param(
        [Parameter(Mandatory=$true)]
        [CxOneConnection]$CxOneConnObj,

        [Parameter(Mandatory=$false)]
        [Switch]$getBranches
    )

    return ([Projects]::new($CxOneConnObj, $getBranches)).ProjectsHash
}

